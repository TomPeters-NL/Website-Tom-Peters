{% set navbarItems = [['about-me', 'About Me'], ['experience', 'Experience'], ['projects', 'Projects'], ['education', 'Education'], ['contact', 'Contact']] %}

<header>
    <div id="header-home">
        <a class="header-icon-link" href="#">
            {{ sigilIcon | safe }}
        </a>
    </div>
    <div id="header-navbar">
        {% for code, title in navbarItems %}
        <div class="header-navigation-item">
            <span class="header-navigation-selector header-navigation-selector-{{code}}">&gt;</span>
            <a class="header-navigation-link" id="header-navigation-link-{{ code }}" href="#{{ code }}">
                {{ title }}
            </a>
            <span class="header-navigation-selector header-navigation-selector-{{code}}">&lt;</span>
        </div>
        {% endfor %}
    </div>
    <div id="header-linkedin">
        <a class="header-icon-link" href="https://www.linkedin.com/in/1992-tom-peters/" target="_blank" rel="noopener noreferrer">
            {{ linkedinIcon | safe }}
        </a>
    </div>
</header>